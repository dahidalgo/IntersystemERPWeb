@model Inspinia_MVC5.Models.SERIE_DOCUMENTO

@{
    ViewBag.Title = "Documentos Series";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Series de Documentos</h2>
    </div>
</div>
<p></p>

<div class="row">
    <div class="col-lg-7 col-md-7 col-sm-10 col-xs-12 col-md-offset-2" col-lg-offset-2">
        <div class="panel panel-default">
            <div class="panel-heading" style="text-align:center">
                <h2>Detalle de Serie</h2>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-2 col-xs-2">
                        <h4><strong> Código:&nbsp;&nbsp; </strong></h4>
                    </div>
                    <div class="col-md-10 col-xs-10">
                        <h4>@Model.SERIE_DOC_ID</h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 col-xs-2">
                        <h4><strong> Serie:&nbsp;&nbsp; </strong></h4>
                    </div>
                    <div class="col-md-10 col-xs-10">
                        <h4>@Model.SERIE</h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 col-xs-2">
                        <h4><strong> Resolución:&nbsp;&nbsp; </strong></h4>
                    </div>
                    <div class="col-md-10 col-xs-10">
                        <h4>@Model.RESOLUCION</h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 col-xs-2">
                        <h4><strong> Desde:&nbsp;&nbsp; </strong></h4>
                    </div>
                    <div class="col-md-10 col-xs-10">
                        <h4>@Model.DESDE</h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 col-xs-2">
                        <h4><strong> Hasta:&nbsp;&nbsp; </strong></h4>
                    </div>
                    <div class="col-md-10 col-xs-10">
                        <h4>@Model.HASTA</h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 col-xs-2">
                        <h4><strong> Fecha de Resolución:&nbsp;&nbsp; </strong></h4>
                    </div>
                    <div class="col-md-10 col-xs-10">
                        <h4>@Model.FECHA_RESOLUCION</h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 col-xs-2">
                        <h4><strong> Fecha Vencimiento:&nbsp;&nbsp; </strong></h4>
                    </div>
                    <div class="col-md-10 col-xs-10">
                        <h4>@Model.FECHA_VIGENTE</h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 col-xs-2">
                        <h4><strong> Estado:&nbsp;&nbsp; </strong></h4>
                    </div>
                    <div class="col-md-10 col-xs-10">
                        <h4>@Model.SERIE_ACTIVO</h4>
                    </div>
                </div>
            </div>
            <div class="panel-footer">
                <div class="row">
                    <div class="col-md-2">
                        <button class="btn btn-success" onclick="location.href = '@Url.Action("Edit", new { id = Model.TIPO_DOC_ID })' "><i class="fad fa-edit"></i>&nbsp;Editar</button>
                    </div>
                    <div class="col-md-3">
                        <button class="btn btn-danger" onclick="location.href = '@Url.Action("Delete", new { id = Model.TIPO_DOC_ID })' "><i class="fad fa-eraser"></i>&nbsp;Eliminar</button>
                    </div>
                    <div class="col-md-3">
                        <button class="btn btn-primary" onclick="location.href = '@Url.Action("Index")' "><i class="fad fa-step-backward"></i>&nbsp;Regresar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>